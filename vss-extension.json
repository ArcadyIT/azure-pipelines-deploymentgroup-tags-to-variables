{
  "manifestVersion": 1,
  "id": "deployment-group-tags-to-variables",
  "name": "Deployment Group Tags to Variables",
  "version": "1.0.1",
  "publisher": "arcady",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "By default, you're not able to use Custom Conditions on Deployment Group Tags. This pipeline step adds the tags of the current deployment group target to your pipeline variables.",
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "files": [
    {
      "path": "buildAndReleaseTask"
    }
  ],
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "buildAndReleaseTask"
      }
    }
  ]
}
